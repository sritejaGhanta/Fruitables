<html>
  <head>
    <!-- Include Bootstrap CSS -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
  </head>
  <body>
    <div class="text-center mb-4 h1"><%= api_name %></div>
    <table class="table table-bordered table-striped">
      <thead class="table-dark">
        <tr>
          <% if (data.length > 0) { %> <% for (const k in data[0]) { %>
          <th><%= k %></th>
          <% } %> <% } %>
        </tr>
      </thead>
      <tbody>
        <% data.forEach(item => { %>
        <tr>
          <% for (const key in item) { %>
          <td><%= item[key] %></td>
          <% } %>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
